<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.menus">
         <menuContribution
            allPopups="false"
            locationURI="menu:window">
            <menu
               id="com.jaspersoft.studio.switchlanguage.menus"
               label="%menu.switchlanguage.label"
               mnemonic="%menu.switchlanguage.mnemonic">
         <command
                  commandId="com.jaspersoft.studio.switchlanguage.command"
                  icon="icons/language/cn.png"
                  id="com.jaspersoft.studio.switchlanguage.menus.chinese"
                  label="%command.switchlanguage.chinese.label"
                  style="toggle">
               <parameter
                     name="com.jaspersoft.studio.switchlanguage.locale"
                     value="zh">
               </parameter>
            </command>
            </menu>
      </menuContribution>
   </extension>

</plugin>
